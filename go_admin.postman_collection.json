					"const statusCode = pm.response.code;",
					"",
					"// 检查状态码是否为 2xx（200-299）",
					"pm.test(\"Success\", function () {",
					"    pm.expect(statusCode).to.be.at.least(200).and.to.be.below(300);",
					"});",
					"",
					"// 处理非 2xx 状态码",
					"if (statusCode < 200 || statusCode >= 300) {",
					"    let errorMessage = \"\";",
					"    ",
					"    // 尝试解析响应体为 JSON",
					"    try {",
					"        const responseJSON = pm.response.json();",
					"        errorMessage = responseJSON.message || responseJSON.error || \"响应体包含 JSON 但无 message 字段\";",
					"    } catch (error) {",
					"        // 响应体非 JSON，直接获取原始文本",
					"        errorMessage = pm.response.text() || \"无响应内容\";",
					"    }",
					"    ",
					"    // 标记测试失败并显示消息",
					"    pm.test(`状态码 ${statusCode}, error: ${errorMessage}`, function () {",
					"        pm.expect(false).to.be.true, errorMessage;",
					"    });",
					"}"
				]
			}
