		{
			"name": "Login",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"// Fixed TypeError by replacing pm.fail with pm.expect",
							"const statusCode = pm.response.code;",
							"pm.test(\"状态码为 200\", function () {",
							"    pm.expect(statusCode).to.eql(200);",
							"});",
							"",
							"// 2. 仅当状态码为 200 时，提取并设置 Token 到 Collection Variables",
							"if (statusCode === 200) {",
							"    try {",
							"        const responseJSON = pm.response.json();",
							"        if (!responseJSON.data) {",
							"            pm.expect.fail(\"响应体缺少 data 字段\");",
							"            return;",
							"        }",
							"        ",
							"        const accessToken = responseJSON.data.access_token;",
							"        const refreshToken = responseJSON.data.refresh_token;",
							"        ",
							"        if (!accessToken || !refreshToken) {",
							"            pm.expect.fail(\"Token 字段为空\");",
							"            return;",
							"        }",
							"        ",
							"        // 设置为 Collection Variables（作用域为当前集合）",
							"        pm.collectionVariables.set(\"college_access_token\", accessToken);",
							"        pm.collectionVariables.set(\"college_refresh_token\", refreshToken);",
							"        ",
							"        pm.test(\"Token 保存到 Collection Variables 成功\", function () {",
							"            pm.expect(pm.collectionVariables.get(\"college_access_token\")).to.not.be.empty;",
							"            pm.expect(pm.collectionVariables.get(\"college_refresh_token\")).to.not.be.empty;",
							"        });",
							"        ",
							"    } catch (error) {",
							"        pm.expect.fail(`解析响应体失败: ${error.message}`);",
							"    }",
							"} else {",
							"    pm.test(\"状态码非 200，不执行 Token 赋值\", function () {",
							"        // 验证 Collection Variables 未被设置",
							"        pm.expect(pm.collectionVariables.get(\"college_access_token\")).to.be.undefined;",
							"        pm.expect(pm.collectionVariables.get(\"college_refresh_token\")).to.be.undefined;",
							"    });",
							"}",
							""
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [],
				"body": {
					"mode": "raw",
					"raw": "{\r\n    \"username\": \"admin\",\r\n    \"password\": \"123456\"\r\n}",
